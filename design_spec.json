{
  "app_analysis": {
    "purpose": "Comprehensive fitness & nutrition tracking dashboard for tracking workouts, meals, body progress, and goals",
    "domain": "fitness",
    "data_density": "medium",
    "primary_users": "Fitness enthusiasts tracking their training and nutrition progress",
    "key_metrics": [
      "Training frequency this week",
      "Calories consumed today",
      "Protein consumed today",
      "Weight trend",
      "Workout streak"
    ],
    "primary_action": "Log workout or nutrition entry",
    "emotional_tone": "energetic and motivating"
  },
  "theme": {
    "mode": "dark",
    "font_family": "Space Grotesk",
    "font_url": "https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@300;500;700&display=swap",
    "colors": {
      "background": "hsl(220, 18%, 8%)",
      "foreground": "hsl(0, 0%, 98%)",
      "primary": "hsl(31, 97%, 58%)",
      "accent": "hsl(178, 65%, 48%)",
      "muted": "hsl(220, 15%, 20%)",
      "positive": "hsl(142, 71%, 45%)",
      "negative": "hsl(0, 72%, 55%)",
      "card": "hsl(220, 16%, 12%)",
      "border": "hsl(220, 15%, 18%)"
    },
    "background_style": "gradient",
    "background_css": "background: linear-gradient(135deg, hsl(220, 18%, 8%) 0%, hsl(220, 20%, 10%) 50%, hsl(220, 18%, 8%) 100%); background-attachment: fixed;"
  },
  "layout": {
    "kpi_cards": [
      {
        "title": "Workouts Diese Woche",
        "source_app": "workouts",
        "calculation": "count_this_week",
        "calculation_field": "datum",
        "icon": "Dumbbell",
        "format": "number",
        "trend_comparison": "vs last week",
        "description": "Trainingseinheiten"
      },
      {
        "title": "Kalorien Heute",
        "source_app": "ernaehrung",
        "calculation": "sum_today",
        "calculation_field": "kalorien",
        "icon": "Flame",
        "format": "number",
        "trend_comparison": "goal_comparison",
        "description": "kcal aufgenommen"
      },
      {
        "title": "Protein Heute",
        "source_app": "ernaehrung",
        "calculation": "sum_today",
        "calculation_field": "protein",
        "icon": "Beef",
        "format": "number",
        "trend_comparison": "goal_comparison",
        "description": "g Protein"
      },
      {
        "title": "Aktuelles Gewicht",
        "source_app": "koerperdaten",
        "calculation": "latest",
        "calculation_field": "gewicht_kg",
        "icon": "Scale",
        "format": "number",
        "trend_comparison": "vs last entry",
        "description": "kg"
      }
    ],
    "main_chart": {
      "type": "area",
      "title": "Gewichtsverlauf (30 Tage)",
      "source_app": "koerperdaten",
      "x_axis": {
        "field": "datum",
        "label": "Datum"
      },
      "y_axis": {
        "field": "gewicht_kg",
        "label": "Gewicht (kg)"
      },
      "group_by": "day",
      "limit_days": 30,
      "color": "hsl(31, 97%, 58%)",
      "gradient": true
      },
    "secondary_sections": [
      {
        "type": "list",
        "title": "Letzte Workouts",
        "source_app": "workouts",
        "fields": [
          "datum",
          "typ",
          "dauer_minuten",
          "stimmung"
        ],
        "sort_by": "datum",
        "sort_order": "desc",
        "limit": 5,
        "show_badges": true
      },
      {
        "type": "table",
        "title": "Heutige Mahlzeiten",
        "source_app": "ernaehrung",
        "fields": [
          "mahlzeit_typ",
          "beschreibung",
          "kalorien",
          "protein"
        ],
        "filter": "today",
        "sort_by": "createdat",
        "sort_order": "asc",
        "show_totals": true
      },
      {
        "type": "cards",
        "title": "Aktive Ziele",
        "source_app": "ziele",
        "fields": [
          "taeglich_kalorien",
          "taeglich_protein",
          "trainingstage_pro_woche"
        ],
        "filter": {
          "field": "status",
          "value": "aktiv"
        },
        "limit": 3,
        "show_progress": true
      }
    ],
    "primary_action_button": {
      "label": "Workout Loggen",
      "action": "navigate",
      "target": "https://my.living-apps.de/app/6914a7e7b773d677cf3838c1",
      "position": "floating",
      "icon": "Plus"
    }
  },
  "animations": {
    "page_load": "stagger",
    "stagger_delay_ms": 80,
    "hover_effects": true,
    "card_hover": "lift"
  },
  "responsive": {
    "mobile_priority": [
      "kpi_cards",
      "primary_action",
      "main_chart"
    ],
    "hide_on_mobile": [],
    "collapse_on_mobile": [
      "secondary_sections"
    ],
    "mobile_navigation": "none"
  }
}

{
  "app_analysis": {
    "purpose": "Comprehensive fitness and nutrition tracking system combining workout logs, meal tracking, body measurements, goals, and exercise library",
    "domain": "fitness",
    "data_density": "medium",
    "primary_users": "Fitness enthusiasts tracking their training progress, nutrition, and body composition",
    "key_metrics": [
      "Weekly workout frequency",
      "Total training time this week",
      "Daily calories consumed vs target",
      "Current weight and body composition trend",
      "Workout mood/quality distribution",
      "Protein intake vs target"
    ],
    "primary_action": "Log today's workout or meal",
    "emotional_tone": "energetic and motivating"
  },
  "theme": {
    "mode": "dark",
    "font_family": "Space Grotesk",
    "font_url": "https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@300;500;700;900&display=swap",
    "colors": {
      "background": "hsl(220 25% 6%)",
      "foreground": "hsl(60 15% 92%)",
      "card": "hsl(220 20% 10%)",
      "border": "hsl(220 15% 18%)",
      "primary": "hsl(25 95% 58%)",
      "accent": "hsl(170 70% 45%)",
      "muted": "hsl(220 15% 25%)",
      "positive": "hsl(142 70% 50%)",
      "negative": "hsl(0 75% 58%)"
    },
    "background_style": "gradient",
    "background_css": "background: linear-gradient(180deg, hsl(220 25% 6%) 0%, hsl(220 30% 4%) 50%, hsl(15 20% 5%) 100%); background-attachment: fixed;"
  },
  "layout": {
    "kpi_cards": [
      {
        "title": "Workouts Diese Woche",
        "source_app": "workouts",
        "calculation": "count",
        "calculation_field": "datum",
        "filter_logic": "current_week_non_rest_days",
        "icon": "Dumbbell",
        "format": "number",
        "trend_comparison": "vs last week",
        "unit": "Trainings"
      },
      {
        "title": "Trainingszeit (Min)",
        "source_app": "workouts",
        "calculation": "sum",
        "calculation_field": "dauer_minuten",
        "filter_logic": "current_week",
        "icon": "Timer",
        "format": "number",
        "trend_comparison": "vs last week",
        "unit": "min"
      },
      {
        "title": "Ø Kalorien Heute",
        "source_app": "ernaehrung",
        "calculation": "sum",
        "calculation_field": "kalorien",
        "filter_logic": "today",
        "icon": "Flame",
        "format": "number",
        "trend_comparison": "vs goal",
        "unit": "kcal",
        "goal_comparison": {
          "source_app": "ziele",
          "goal_field": "taeglich_kalorien",
          "show_progress_bar": true
        }
      },
      {
        "title": "Aktuelles Gewicht",
        "source_app": "koerperdaten",
        "calculation": "latest",
        "calculation_field": "gewicht_kg",
        "icon": "Scale",
        "format": "number",
        "trend_comparison": "vs last entry",
        "unit": "kg",
        "precision": 1
      }
    ],
    "main_chart": {
      "type": "area",
      "title": "Workouts & Stimmung (Letzte 30 Tage)",
      "source_app": "workouts",
      "x_axis": {
        "field": "datum",
        "label": "Datum"
      },
      "y_axis": {
        "field": "dauer_minuten",
        "label": "Dauer (Min)"
      },
      "group_by": "day",
      "additional_data": {
        "mood_indicators": {
          "field": "stimmung",
          "display": "color_coded_markers"
        },
        "rest_days": {
          "field": "rest_day",
          "display": "gray_background"
        }
      },
      "date_range": "last_30_days"
    },
    "secondary_sections": [
      {
        "type": "progress_rings",
        "title": "Heutige Ernährung",
        "layout": "horizontal_cards",
        "metrics": [
          {
            "label": "Kalorien",
            "source_app": "ernaehrung",
            "actual_field": "kalorien",
            "goal_app": "ziele",
            "goal_field": "taeglich_kalorien",
            "filter_logic": "today",
            "format": "kcal"
          },
          {
            "label": "Protein",
            "source_app": "ernaehrung",
            "actual_field": "protein",
            "goal_app": "ziele",
            "goal_field": "taeglich_protein",
            "filter_logic": "today",
            "format": "g"
          }
        ]
      },
      {
        "type": "mini_chart",
        "title": "Gewichtsverlauf (90 Tage)",
        "chart_type": "line",
        "source_app": "koerperdaten",
        "x_field": "datum",
        "y_field": "gewicht_kg",
        "date_range": "last_90_days",
        "show_trend_line": true
      },
      {
        "type": "list",
        "title": "Letzte Workouts",
        "source_app": "workouts",
        "fields": [
          "datum",
          "typ",
          "dauer_minuten",
          "stimmung"
        ],
        "display_format": "compact_cards",
        "sort_by": "datum",
        "sort_order": "desc",
        "limit": 5,
        "show_icons": true
      },
      {
        "type": "stats_grid",
        "title": "Workout-Typ Verteilung (30 Tage)",
        "source_app": "workouts",
        "group_by": "typ",
        "calculation": "count",
        "date_range": "last_30_days",
        "display": "horizontal_bars"
      }
    ],
    "primary_action_button": {
      "label": "Workout Eintragen",
      "action": "add_record",
      "target_app": "workouts",
      "position": "header",
      "icon": "Plus",
      "variant": "default"
    }
  },
  "animations": {
    "page_load": "stagger",
    "stagger_delay_ms": 80,
    "hover_effects": true,
    "card_hover": "lift"
  },
  "responsive": {
    "mobile_priority": [
      "kpi_cards",
      "primary_action",
      "main_chart",
      "progress_rings"
    ],
    "hide_on_mobile": [],
    "mobile_navigation": "none",
    "kpi_cards_mobile": "2x2_grid",
    "kpi_cards_desktop": "4_columns"
  },
  "custom_styling": {
    "card_style": "glass_subtle",
    "chart_colors": {
      "primary": "hsl(25 95% 58%)",
      "secondary": "hsl(170 70% 45%)",
      "mood_schlecht": "hsl(0 75% 58%)",
      "mood_okay": "hsl(40 90% 55%)",
      "mood_gut": "hsl(142 70% 50%)",
      "mood_brutal": "hsl(170 80% 50%)",
      "rest_day": "hsl(220 15% 15%)"
    },
    "kpi_card_accents": true,
    "progress_bar_height": "8px"
  }
}

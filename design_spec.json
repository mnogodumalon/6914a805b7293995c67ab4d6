{
  "app_analysis": {
    "purpose": "Personal fitness and nutrition tracking dashboard for tracking workouts, meals, body metrics, and progress toward goals",
    "domain": "fitness",
    "data_density": "medium",
    "primary_users": "Fitness enthusiasts tracking their training, nutrition, and body composition progress",
    "key_metrics": [
      "Total workouts completed",
      "Training consistency (workout frequency)",
      "Daily calorie and protein intake vs goals",
      "Body weight trend",
      "Training volume and intensity"
    ],
    "primary_action": "Quick log workout or meal entry",
    "emotional_tone": "energetic and motivating - users want to feel pumped and see clear progress"
  },
  "theme": {
    "mode": "dark",
    "font_family": "Space Grotesk",
    "font_url": "https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@300;500;700;800&display=swap",
    "colors": {
      "background": "hsl(220 25% 6%)",
      "foreground": "hsl(220 10% 94%)",
      "card": "hsl(220 20% 10%)",
      "border": "hsl(220 15% 18%)",
      "primary": "hsl(25 95% 58%)",
      "accent": "hsl(165 80% 50%)",
      "muted": "hsl(220 15% 20%)",
      "positive": "hsl(142 70% 50%)",
      "negative": "hsl(0 70% 55%)"
    },
    "background_style": "radial gradient with subtle grid pattern",
    "background_css": "background: radial-gradient(ellipse at top, hsl(220 30% 10%) 0%, hsl(220 25% 6%) 60%); background-image: radial-gradient(ellipse at top, hsl(220 30% 10%) 0%, hsl(220 25% 6%) 60%), linear-gradient(rgba(255,255,255,0.015) 1px, transparent 1px), linear-gradient(90deg, rgba(255,255,255,0.015) 1px, transparent 1px); background-size: 100% 100%, 24px 24px, 24px 24px;"
  },
  "layout": {
    "kpi_cards": [
      {
        "title": "Workouts Diese Woche",
        "source_app": "workouts",
        "calculation": "count_this_week",
        "calculation_field": "datum",
        "icon": "Dumbbell",
        "format": "number",
        "suffix": " Sessions",
        "trend_comparison": "vs last week"
      },
      {
        "title": "Kalorien Heute",
        "source_app": "ernaehrung",
        "calculation": "sum_today",
        "calculation_field": "kalorien",
        "icon": "Flame",
        "format": "number",
        "suffix": " kcal",
        "trend_comparison": "vs goal"
      },
      {
        "title": "Protein Heute",
        "source_app": "ernaehrung",
        "calculation": "sum_today",
        "calculation_field": "protein",
        "icon": "Beef",
        "format": "number",
        "suffix": "g",
        "trend_comparison": "vs goal"
      },
      {
        "title": "Aktuelles Gewicht",
        "source_app": "koerperdaten",
        "calculation": "latest",
        "calculation_field": "gewicht_kg",
        "icon": "TrendingUp",
        "format": "number",
        "suffix": " kg",
        "trend_comparison": "vs last month"
      }
    ],
    "main_chart": {
      "type": "area",
      "title": "Trainingsvolumen (letzte 4 Wochen)",
      "source_app": "workouts",
      "x_axis": {
        "field": "datum",
        "label": "Datum"
      },
      "y_axis": {
        "field": "dauer_minuten",
        "label": "Dauer (Min)"
      },
      "group_by": "day",
      "additional_info": "Show workout duration over time with distinct colors for workout types (typ field)"
    },
    "secondary_sections": [
      {
        "type": "list",
        "title": "Letzte Workouts",
        "source_app": "workouts",
        "fields": [
          "datum",
          "typ",
          "dauer_minuten",
          "stimmung"
        ],
        "sort_by": "datum",
        "sort_order": "desc",
        "limit": 5
      },
      {
        "type": "cards",
        "title": "Aktive Ziele",
        "source_app": "ziele",
        "filter": {
          "field": "status",
          "value": "aktiv"
        },
        "fields": [
          "taeglich_kalorien",
          "taeglich_protein",
          "trainingstage_pro_woche",
          "schlaf_ziel_stunden"
        ],
        "limit": 3
      },
      {
        "type": "chart",
        "title": "KÃ¶rpergewicht Trend (30 Tage)",
        "chart_type": "line",
        "source_app": "koerperdaten",
        "x_axis": {
          "field": "datum",
          "label": "Datum"
        },
        "y_axis": {
          "field": "gewicht_kg",
          "label": "Gewicht (kg)"
        },
        "group_by": "day"
      }
    ],
    "primary_action_button": {
      "label": "Workout loggen",
      "action": "navigate",
      "target_url": "https://my.living-apps.de/workspaces/6914a805b7293995c67ab4d6",
      "position": "header",
      "variant": "default"
    }
  },
  "animations": {
    "page_load": "stagger",
    "stagger_delay_ms": 80,
    "hover_effects": true,
    "card_hover": "lift"
  },
  "responsive": {
    "mobile_priority": [
      "kpi_cards",
      "primary_action",
      "main_chart"
    ],
    "hide_on_mobile": [],
    "mobile_navigation": "none",
    "mobile_notes": "Stack all sections vertically on mobile, show 2 KPI cards per row on small screens, 4 on desktop"
  }
}

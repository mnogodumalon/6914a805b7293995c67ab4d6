  // --- ERNAEHRUNG ---
  static async getErnaehrung(): Promise<Ernaehrung[]> {
    const data = await callApi('GET', `/apps/${APP_IDS.ERNAEHRUNG}/records`);
    return Object.entries(data).map(([id, rec]: [string, any]) => ({

    satz_nummer?: number;
    gewicht?: number;
    wiederholungen?: number;
    rpe?: 'rpe_1' | 'rpe_2' | 'rpe_3' | 'rpe_4' | 'rpe_5' | 'rpe_6' | 'rpe_7' | 'rpe_8' | 'rpe_9' | 'rpe_10';
